  "scripts": {
    "start": "npm run start:all",
    "start:admin-portal": "npx nx serve orchestrix-admin --port=26443 --ssl --ssl-cert ./ssl/localhost.crt --ssl-key ./ssl/localhost.key",
    "start:public-portal": "npx nx serve orchestrix-public --port=25443 --ssl --ssl-cert ./ssl/localhost.crt --ssl-key ./ssl/localhost.key",
    "start:all": "concurrently -c \"blue,green\" \"npm run start:admin-portal\" \"npm run start:public-portal\"",
    "build": "npm run build:all",
    "build:admin-portal": "npx nx build orchestrix-admin",
    "build:public-portal": "npx nx build orchestrix-public",
    "build:all": "concurrently -c \"blue,green\" \"npm run build:admin-portal\" \"npm run build:public-portal\"",
    "test": "npm run test:all",
    "test:admin-portal": "npx nx test orchestrix-admin",
    "test:public-portal": "npx nx test orchestrix-public",
    "test:all": "concurrently -c \"blue,green\" \"npm run test:admin-portal\" \"npm run test:public-portal\"",
    "lint": "npm run lint:all",
    "lint:admin-portal": "npx nx lint orchestrix-admin",
    "lint:public-portal": "npx nx lint orchestrix-public",
    "lint:all": "concurrently -c \"blue,green\" \"npm run lint:admin-portal\" \"npm run lint:public-portal\""
  },
  "private": true,
